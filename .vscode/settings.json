{
    "chat.mcp.serverSampling": {
        "med-paper-assistant/.vscode/mcp.json: mdpaper": {
            "allowedModels": [
                "copilot/claude-opus-4.5"
            ]
        }
    },
    "foam.edit.linkReferenceDefinitions": "withExtensions",
    "foam.links.hover.enable": true,
    "foam.links.sync.enable": true,
    "foam.openDailyNote.directory": "notes",
    "foam.files.ignore": [
        ".venv/**",
        "node_modules/**",
        ".git/**",
        "__pycache__/**",
        "integrations/**",
        "tests/**",
        "dashboard/**",
        "memory-bank/**",
        "templates/**",
        "scripts/**",
        ".claude/**",
        ".github/**",
        "src/**",
        "docs/**",
        "*.md",
        "*.json",
        "*.toml",
        "*.yaml",
        "*.yml",
        "*.txt",
        "*.lock",
        "projects/airway-device-oral-lip-injury-comparison/concept.md",
        "projects/airway-device-oral-lip-injury-comparison/project.json",
        "projects/airway-device-oral-lip-injury-comparison/.memory/**",
        "projects/airway-device-oral-lip-injury-comparison/drafts/**",
        "projects/airway-device-oral-lip-injury-comparison/data/**",
        "projects/airway-device-oral-lip-injury-comparison/results/**",
        "projects/test-isolation-project/**"
    ],
    "foam.completion.label": "title",
    "foam.graph.titleMaxLength": 50,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
    },
    "[markdown]": {
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.wordBasedSuggestions": "off"
    }
}